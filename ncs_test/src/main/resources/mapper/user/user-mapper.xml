<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mvc.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="user">
    <select id="findById" resultType="user">
        select
            *
        from
            tb_user
        where
            user_id = #{userId}
    </select>
</mapper>