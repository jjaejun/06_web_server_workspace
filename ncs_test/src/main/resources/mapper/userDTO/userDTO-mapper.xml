<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mvc.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="userDTO">
    <select id="findByNo" resultType="userDTO">
        select
            *
        from
            tb_user
        where
            user_no = #{userNo}
    </select>
</mapper>